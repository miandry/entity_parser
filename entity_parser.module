<?php

/**
 * @file
 * Contains entity_parser.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function entity_parser_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the entity_parser module.
    case 'help.page.entity_parser':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Change Entity format to array , json ,...') . '</p>';
      return $output;

    default:
  }
}


